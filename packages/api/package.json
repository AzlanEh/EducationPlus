{
	"name": "@eduPlus/api",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./src/index.ts"
		},
		"./routers": {
			"types": "./dist/routers/index.d.ts",
			"default": "./src/routers/index.ts"
		},
		"./routers/v1": {
			"types": "./dist/routers/v1/index.d.ts",
			"default": "./src/routers/v1/index.ts"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"default": "./src/*.ts"
		}
	},
	"type": "module",
	"scripts": {
		"build": "tsdown"
	},
	"devDependencies": {
		"tsdown": "catalog:",
		"@eduPlus/config": "workspace:*"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@orpc/server": "catalog:",
		"@orpc/client": "catalog:",
		"@orpc/openapi": "catalog:",
		"@orpc/zod": "catalog:",
		"hono": "catalog:",
		"dotenv": "catalog:",
		"zod": "catalog:",
		"@eduPlus/auth": "workspace:*",
		"@eduPlus/db": "workspace:*"
	}
}
